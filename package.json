{
  "name": "sketch-seu",
  "version": "0.1.1",
  "description": "a simple plugin",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-seu",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wwsun/sketch-seu.git"
  },
  "author": "jingzhuang.sww <jingzhuang.sww@alibaba-inc.com>"
}
